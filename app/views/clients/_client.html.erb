<div id="<%= dom_id client %>">
  <p>
    <strong>First name:</strong>
    <%= client.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= client.last_name %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= client.phone %>
  </p>

  <p>
    <strong>Bio:</strong>
    <%= client.bio %>
  </p>

  <p>
    <strong>Users:</strong>
    <%= client.users_id %>
  </p>

</div>
